<Window x:Class="JewelleryManagementSystem.Settings.View.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JewelleryManagementSystem.Settings.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner" Background="#f1d9fa" Height="700" Width="1050" ResizeMode="CanMinimize"
        ShowInTaskbar="False">
    <Grid>
        <TabControl x:Name="tabControl" Background="#f1d9fa">
            <TabItem x:Name="settingsTab" Height="30" Width="120" Header="Settings">
            </TabItem>
            <TabItem x:Name="metalTab" Height="30" Width="120" Header="Add Metal">
            </TabItem>
            <TabItem x:Name="ornamentTab" Height="30" Width="120" Header="Add Ornament"></TabItem>
        </TabControl>
    </Grid>
</Window>
