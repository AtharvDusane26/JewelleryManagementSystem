<Window x:Class="JewelleryManagementSystem.Settings.View.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JewelleryManagementSystem.Settings.View"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen" Background="#2C2C2C" Height="700" Width="1050" ResizeMode="CanMinimize"
        ShowInTaskbar="True">
    <Grid>
        <TabControl x:Name="tabControl" Background="#2C2C2C">
            <!--<TabItem x:Name="settingsTab" Height="30" Width="120" Header="Settings">
            </TabItem>-->
            <TabItem x:Name="metalTab" Height="30" Width="120" Header="Metal Settings">
            </TabItem>
            <TabItem x:Name="ornamentTab" Height="30" Width="120" Header="Ornament Settings"></TabItem>
            <TabItem x:Name="stockTab" Height="30" Width="120" Header="Stock Settings"></TabItem>
            <TabItem x:Name="generalSettingTab" Height="30" Width="120" Header="General Settings"></TabItem>
        </TabControl>
    </Grid>
</Window>
